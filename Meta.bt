typedef struct(FourCC type, uint64 size)
{
  local uint64 start <format=hex, hidden=true> = FTell();
  switch (type)
  {
      case "TXTR":
          CTextureMeta meta;
          break;
      default:
          byte data[size];
          break;
  }
  FSeek(start + size);
} Meta <open=true, size=size>;
