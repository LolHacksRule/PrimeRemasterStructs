struct Material
{
    CStringFixed name;
};


struct ModelChunk
{
    ChunkDescriptor chunk;

	local uint pos <format=hex, hidden=true> = FTell();
    switch (chunk.id)
    {
        case "MTRL":
            uint unk;
            uint numMaterials;
            Material materials[numMaterials] <optimize=false>;
            break;
        case "MESH":
            break;
    }
    FSeek(pos + chunk.size);
};
